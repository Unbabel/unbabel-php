{
    "name": "unbabel/unbabel-php",
    "type": "library",
    "description": "Unbabel PHP SDK",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Unbabel Tech Team",
            "email": "tech-support@unbabel.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5 || ^7.0"
    },
    "autoload": {
        "psr-0": {
            "Unbabel\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Unbabel\\Tests\\": "Tests/"
        }
    }
}
